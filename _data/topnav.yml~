          external_url: http://hpcc.ucr.edu/
        - title: Girke Lab
          external_url: http://girke.bioinformatics.ucr.edu/
        - title: Jekyll Doc Theme
          external_url: http://idratherbewriting.com/documentation-theme-jekyll
